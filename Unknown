      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "\n",
        "nfa = {}\n",
        "n = int(input(\"No. of states : \"))\n",
        "t = int(input(\"No. of transitions : \"))\n",
        "for i in range(n):\n",
        "    state = input(\"state name : \")\n",
        "    nfa[state] = {}\n",
        "    for j in range(t):\n",
        "        path = input(\"path : \")\n",
        "        nfa[state][path] = input(\n",
        "            f\"Enter end state from state {state} travelling through path {path} : \").split()\n",
        "\n",
        "print(\"\\n\\nNFA :\")\n",
        "nfa_table = pd.DataFrame(nfa)\n",
