        "def clean(token):\n",
        "    return ''.join(c for c in token if c.isalnum() or c == '.')\n",
        "\n",
        "\n",
        "with open('input.c') as f:\n",
        "    c = f.read()\n",
        "    tokens = c.split()\n",
        "    print(f'total tokens = {len(tokens)}', end='\\n\\n')\n",
        "\n",
        "    keywords = ['int', '#include', 'return']\n",
        "\n",
        "    for token in tokens:\n",
        "        match token:\n",
        "            case token if token in keywords:\n",
        "                print(f'<keyword {token}>')\n",
        "            case '{':\n",
        "                print('<symbol {>')\n",
        "            case '}':\n",
        "                print('<symbol }>')\n",
        "            case '+':\n",
        "                print('<symbol +>')\n",
        "            case '=':\n",
        "                print('<symbol =>')\n",
        "            case token if token[0].isdigit():\n",
        "                print(f'<literal {token[0]}>')\n",
        "            case token if token[0] == '\"':\n",
        "                print(f'<literal {token[1:-2]}>')\n",
        "            case _:\n",
        "                print(f'<identifier {clean(token)}>')"
