      "cell_type": "code",
      "source": [
        "grammar = {\n",
        "    \"S\": [\"S*S\", \"S+S\", \"i\"]\n",
        "}\n",
        "starting_terminal = \"S\"\n",
        "inp = \"i+i+i$\"\n",
        "stack = \"$\"\n",
        "\n",
        "print(' | '.join(\n",
        "    [\"Stack\".ljust(15), \"Input Buffer\".ljust(15), 'Parsing Action']))\n",
        "print('-'*50)\n",
        "\n",
        "while True:\n",
        "    action = True\n",
        "    i = 0\n",
        "    while i < len(grammar[starting_terminal]):\n",
        "        if grammar[starting_terminal][i] in stack:\n",
        "            stack = stack.replace(\n",
        "                grammar[starting_terminal][i], starting_terminal)\n",
        "            print(' | '.join([stack.ljust(15), inp.ljust(15),\n",
        "                  f'Reduce S->{grammar[starting_terminal][i]}']))\n",
        "            i = -1\n",
        "            action = False\n",
        "        i += 1\n",
        "        \n",
        "    if len(inp) > 1:\n",
        "        print(' | '.join([stack.ljust(15), inp.ljust(15), 'Shift']))\n",
        "        stack += inp[0]\n",
        "        inp = inp[1:]\n",
        "        action = False\n",
        "\n",
        "    if inp == \"$\" and stack == (\"$\"+starting_terminal):\n",
        "        print(' | '.join([stack.ljust(15), inp.ljust(15), 'Accept']))\n",
        "        break\n",
        "\n",
        "    if action:\n",
        "        print(' | '.join([stack.ljust(15), inp.ljust(15), 'Reject']))\n",
        "        break\n"
      ],
