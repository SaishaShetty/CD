        "grammar = \"\"\"\n",
        "S -> a A B e\n",
        "A -> b C\n",
        "C -> b c C | #\n",
        "B -> d\n",
        "\"\"\"\n",
        "\n",
        "\n",
        "def parse_grammar(grammar):\n",
        "    g = grammar.strip().splitlines()\n",
        "    p = [s.split(' -> ') for s in g]\n",
        "    k = [(l, r.split(' | ')) for l, r in p]\n",
        "    return {l: [s.split() for s in r] for l, r in k}\n",
        "\n",
        "\n",
        "productions = parse_grammar(grammar)\n",
        "\n",
        "\n",
        "def find_first(nt):\n",
        "    if nt.isupper():\n",
        "        return first(nt)[0]\n",
        "    else:\n",
        "        return nt\n",
        "\n",
        "\n",
        "def first(nt):\n",
        "    f = []\n",
        "\n",
        "    for prod in productions[nt]:\n",
        "        f += find_first(prod[0])\n",
        "\n",
        "    return f\n",
        "\n",
        "\n",
        "def find_follow(prod):\n",
        "    lt = prod[-1]\n",
        "    if lt.isupper():\n",
        "        return find_follow(prod[-2])\n",
        "    else:\n",
        "        return lt\n",
        "\n",
        "\n",
        "def follow(nt):\n",
        "    f = []\n",
        "\n",
        "    for prod in productions[nt]:\n",
        "        f += find_follow(prod)\n",
        "\n",
        "    return f\n",
        "\n",
        "\n",
        "for l, r in productions.items():\n",
        "    print(f'First({l}) = {first(l)}')\n",
        "print()\n",
        "\n",
        "for l, r in productions.items():\n",
        "    print(f'Follow({l}) = {follow(l)}')\n"
      ],
