      "cell_type": "code",
      "source": [
        "grammar = \"\"\"\n",
        "E -> E + T | T\n",
        "T -> T * F | F\n",
        "F -> ( E ) | i\n",
        "\"\"\"\n",
        "\n",
        "\n",
        "def parse_grammar(grammar):\n",
        "    g = grammar.strip().splitlines()\n",
        "    p = [s.split(' -> ') for s in g]\n",
        "    k = [(l, r.split(' | ')) for l, r in p]\n",
        "    return {l: [s.split() for s in r] for l, r in k}\n",
        "\n",
        "\n",
        "def remove_left_recursion(productions):\n",
        "    prods = {k: [] for k in productions.keys()}\n",
        "    for lhs, rhss in productions.items():\n",
        "        rec = len(rhss) > 1 and len(set([rhs[-1] for rhs in rhss])) == 1\n",
        "        if rec:\n",
        "            c = rhss[0][-1]\n",
        "            nlhs = lhs + \"'\"\n",
        "            prods[lhs] = [[c, nlhs]]\n",
        "            prods[nlhs] = [[*rhss[0][1:], nlhs], '#']\n",
        "        else:\n",
        "            prods[lhs] = rhss\n",
        "\n",
        "    return prods\n",
        "\n",
        "\n",
        "productions = parse_grammar(grammar)\n",
        "lr = remove_left_recursion(productions)\n",
        "for l, r in lr.items():\n",
        "    print(l, '->', ' | '.join([' '.join(s) for s in r]))"
      ],
