      "cell_type": "code",
      "source": [
        "grammar = \"\"\"\n",
        "S -> i E t S | i E t S e S | a\n",
        "E -> b\n",
        "\"\"\"\n",
        "\n",
        "\n",
        "def common_prefix(lst):\n",
        "    c = []\n",
        "    for s in zip(*lst):\n",
        "        if len(set(s)) != 1:\n",
        "            break\n",
        "        c += s[0]\n",
        "    return c\n",
        "\n",
        "\n",
        "def parse_grammar(grammar):\n",
        "    g = grammar.strip().splitlines()\n",
        "    p = [s.split(' -> ') for s in g]\n",
        "    k = [(l, r.split(' | ')) for l, r in p]\n",
        "    return {l: [s.split() for s in r] for l, r in k}\n",
        "\n",
        "\n",
        "def remove_left_factoring(productions):\n",
        "    prods = {k: [] for k in productions.keys()}\n",
        "    for lhs, rhss in productions.items():\n",
        "        if len(rhss) > 1:\n",
        "            cm = [rhs for rhs in rhss if len(rhs) > 1]\n",
        "            rest = [rhs for rhs in rhss if len(rhs) == 1]\n",
        "\n",
        "            cp = common_prefix(cm)\n",
        "            mcm = max(cm)[len(cp):]\n",
        "\n",
        "            nlhs = lhs + \"'\"\n",
        "            prods[lhs] = [[*cp, nlhs], *rest]\n",
        "            prods[nlhs] = [mcm, ['#']]\n",
        "        else:\n",
        "            prods[lhs] = rhss\n",
        "\n",
        "    return prods\n",
        "\n",
        "\n",
        "productions = parse_grammar(grammar)\n",
        "lr = remove_left_factoring(productions)\n",
        "for l, r in lr.items():\n",
        "    print(l, '->', ' | '.join([' '.join(s) for s in r]))\n"
      ],
      "metadata": {
