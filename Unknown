      "cell_type": "code",
      "source": [
        "from collections import deque                 #prefix\n",
        "def parse(tokens):\n",
        "    token=tokens.popleft()\n",
        "    if token=='+':\n",
        "            return parse(tokens)+parse(tokens)\n",
        "    elif token=='-':\n",
        "            return parse(tokens)-parse(tokens)\n",
        "    elif token=='*':\n",
        "            return parse(tokens)*parse(tokens)\n",
        "    elif token=='/':\n",
        "            return parse(tokens)/parse(tokens)\n",
        "    else:\n",
        "            # must be just a number\n",
        "            return int(token)\n",
        "\n",
        "\n",
        "if __name__=='__main__':\n",
        "        expression=\"2 3 1 * + 9 -\"\n",
        "        print(parse(deque(expression.split())))"
      ],
