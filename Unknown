      "cell_type": "code",
      "source": [
        "grammar = \"\"\"\n",
        "S -> a | ^ | ( T )\n",
        "T -> T , S | S\n",
        "\"\"\"\n",
        "print(grammar)\n",
        "\n",
        "def parse_gramar(grammar):\n",
        "    g = grammar.strip().splitlines()\n",
        "    p = [s.split(' -> ') for s in g]\n",
        "    k = [(l, r.split(' | ')) for l, r in p]\n",
        "    return { l: [s.split() for s in r] for l, r in k }\n",
        "    \n",
        "grammar = parse_gramar(grammar)\n",
        "nts = list(grammar.keys())\n",
        "\n",
        "def leading(nt):\n",
        "    prods = grammar[nt]\n",
        "    l = set()\n",
        "    for p in prods:\n",
        "        ft = p[0]\n",
        "        if ft == nt:\n",
        "            ft = p[1]\n",
        "        if ft in nts:\n",
        "            l |= leading(ft)\n",
        "        else:\n",
        "            l.add( ft )\n",
        "    return l\n",
        "    \n",
        "#\n",
        "def trailing(nt):\n",
        "    prods = grammar[nt]\n",
        "    l = set()\n",
        "    for p in prods:\n",
        "        ft = p[-1]\n",
        "\t\n",
        "        if ft == nt or ft == nts[0]:\n",
        "            if len(p) > 1:\n",
        "                ft = p[-2]\n",
        "        if ft in nts:\n",
        "            l |= trailing(ft)\n",
        "        else:\n",
        "            l.add( ft )\n",
        "    return l\n",
        "\n",
        "\n",
        "for nt in nts:\n",
        "    print(f\"Leading({nt}) = {leading(nt)}\")\n",
        "print()\n",
        "for nt in nts:\n",
        "    print(f\"Trailing({nt}) = {trailing(nt)}\")"
      ],
