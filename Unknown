        "from collections import deque            #postfix\n",
        "\n",
        "ex = '231*+9-'\n",
        "q = deque()\n",
        "\n",
        "for i in ex:\n",
        "    if i.isdigit():\n",
        "        q.append(i)\n",
        "    else:\n",
        "        a = q.pop()\n",
        "        b = q.pop()\n",
        "        q.append(str(eval(a + i + b)))\n",
        "    print('debug:', i, q)               # Just add this line.\n",
